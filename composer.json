{
    "name": "laravel-shine/validator-extension",
    "version": "1.0.0",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Albert",
            "email": "albertdevtw@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/translation": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/validation": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelShine\\ValidatorExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelShine\\ValidatorExtension\\ValidatorExtensionServiceProvider"
            ]
        }
    }
}
